import sys
import time
import webbrowser





def main():

	total_breaks=3
	break_count=0

	print('This program started on '+time.ctime())

	#for x in range(0, 3):
	while(break_count<total_breaks):
		time.sleep(10)
		webbrowser.open('https://www.youtube.com/watch?v=fGKVpTRe8GM')
		break_count=break_count+1

if __name__ == '__main__':
    main()